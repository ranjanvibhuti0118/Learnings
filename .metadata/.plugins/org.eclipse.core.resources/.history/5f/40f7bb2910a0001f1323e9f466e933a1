package com.app.order.events;

import java.util.List;

import com.app.order.entity.Product;

public class OrderCreatedEvent{
	
	private Long orderId;
	private List<Product> products;
	
	public Long getOrderId() {
		return orderId;
	}
	public void setOrderId(Long orderId) {
		this.orderId = orderId;
	}
	public List<Product> getProductIds() {
		return products;
	}
	public void setProductIds(List<Product> products) {
		this.products = products;
	}
	public OrderCreatedEvent(Long orderId, List<Product> products) {
		super();
		this.orderId = orderId;
		this.products = products;
	}
	public OrderCreatedEvent() {
		super();
		// TODO Auto-generated constructor stub
	}
	

	
}
